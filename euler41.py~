#!/usr/bin/python

def isPandigital(x):
    digitLimit = len(str(x))
    for i in range(1, digitLimit + 1):
	if str(i) not in str(x):
	    print str(i)
	    return False
    return True

def isPrime(n):
    n = abs(int(n))
    if n < 2:
	return False
    if n == 2:
	return True
    if not n & 1:
	return False
    for x in range(3, int(n**0.5)+1, 2):
	if n % x == 0:
	    return False
    return True

maxP = 0
i = 1
while len(str(i)) <= 9:
    if isPandigital(i) and isPrime(i):
	maxP = i
    i += 2

print str(maxP)
