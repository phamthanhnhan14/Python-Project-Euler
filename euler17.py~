#!/usr/bin/python

# idea: use len("two), etc

def numLetters(x):
    # irregular and base cases
    if x < 20:
	if x == 0:
	    return 0
	if x == 1 or x == 2 or x == 6 or x == 10:
	    return 3
	if x == 4 or x == 5 or x == 9:
	    return 4
	if x == 3 or x == 7 or x == 8:
	    return 5
	if x == 11 or x == 12:
	    return 6
	if x == 15 or x == 16:
	    return 7
	if x == 13 or x == 14 or x == 18 or x == 19:
	    return 8
	if x == 17:
	    return 9
    # less than one hundred
    elif x < 100:
	if x >= 90:
	    return 6 + numLetters(x-90)
	if x >= 80:
	    return 6 + numLetters(x-80)
	if x >= 70:
	    return 7 + numLetters(x-70)
	if x >= 60:
	    return 5 + numLetters(x-60)
	if x >= 50:
	    return 5 + numLetters(x-50)
	if x >= 40:
	    return 5 + numLetters(x-40)
	if x >= 30:
	    return 6 + numLetters(x-30)
	if x >= 20:
	    return 6 + numLetters(x-20)
    # less than one thousand
    elif x < 1000:
	if x > 900:
	    return numLetters(x//100) + 10 + numLetters(x-900)
	if x == 900:
	    return numLetters(x//100) + 7
	if x > 800:
	    return numLetters(x//100) + 10 + numLetters(x-800)
	if x == 800:
	    return numLetters(x//100) + 7
	if x > 700:
	    return numLetters(x//100) + 10 + numLetters(x-700)
	if x == 700:
	    return numLetters(x//100) + 7
	if x > 600:
	    return numLetters(x//100) + 10 + numLetters(x-600)
	if x == 600:
	    return numLetters(x//100) + 7
	if x > 500:
	    return numLetters(x//100) + 10 + numLetters(x-500)
	if x == 500:
	    return numLetters(x//100) + 7
	if x > 400:
	    return numLetters(x//100) + 10 + numLetters(x-400)
	if x == 400:
	    return numLetters(x//100) + 7
	if x > 300:
	    return numLetters(x//100) + 10 + numLetters(x-300)
	if x == 300:
	    return numLetters(x//100) + 7
	if x > 200:
	    return numLetters(x//100) + 10 + numLetters(x-200)
	if x == 200:
	    return numLetters(x//100) + 7
	if x > 100:
	    return numLetters(x//100) + 10 + numLetters(x-100)
	if x == 100:
	    return numLetters(x//100) + 7
    elif x == 1000:
	return numLetters(x//1000) + 8

totalLetters = 0
for i in range (1, 1001):
    totalLetters += numLetters(i)
print str(totalLetters)
    
