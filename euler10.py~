def isprime(n):
    n = abs(int(n))
    if n < 2:
	return False
    if n == 2:
	return True
    if not n & 1:
	return False
    for x in range(3, int(n**0.5)+1, 2):
	if n % x == 0:
	    return False
    return True

primes = 0
current = 1
while primes < 10001:
    current += 1
    if isprime(current):
	primes += 1


print current
